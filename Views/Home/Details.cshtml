@model DetailedViewItem
@{
    Layout = "_Layout";
}

<p class="name">
    @Model.Name
</p>
<span>Links:</span>
<div class="links">
    @foreach (var link in Model.Links)
    {
        <a href="@link" style="display: block;">Click</a>
    }
</div>

@section Head{
<link rel="stylesheet" href="~/css/detailStyles.css" />
}